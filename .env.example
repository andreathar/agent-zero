# =============================================================================
# Agent Zero Environment Configuration
# =============================================================================
# Copy this file to .env and fill in your API keys
# Required: At least one LLM provider API key
# =============================================================================

# ============================================
# LLM Provider API Keys (choose at least one)
# ============================================

# Google Gemini (recommended for Agent Zero)
GEMINI_API_KEY=your_gemini_api_key_here

# OpenAI
OPENAI_API_KEY=your_openai_api_key_here

# Anthropic Claude
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# OpenRouter (access to multiple models)
OPENROUTER_API_KEY=your_openrouter_api_key_here

# ============================================
# Model Configuration
# ============================================

# Chat Model (main reasoning model)
CHAT_MODEL_PROVIDER=google
CHAT_MODEL_NAME=gemini-2.5-pro

# Utility Model (fast tasks, code generation)
UTIL_MODEL_PROVIDER=google
UTIL_MODEL_NAME=gemini-2.5-flash

# Embeddings Model
EMBEDDINGS_MODEL_PROVIDER=HuggingFace
EMBEDDINGS_MODEL_NAME=sentence-transformers/all-MiniLM-L6-v2

# ============================================
# Unity Configuration
# ============================================

# Unity project name (folder name in usr/projects/)
UNITY_PROJECT_NAME=UnityProject

# Unity Editor version
UNITY_EDITOR_VERSION=6000.2.10f1

# ============================================
# Qdrant Configuration
# ============================================

# Qdrant URL (for agent use, internal Docker network)
QDRANT_URL=http://qdrant-unity:6333

# Qdrant API Key (leave empty for local deployment)
QDRANT_API_KEY=

# Default collection name
A0_QDRANT_COLLECTION=agent-zero-unity

# ============================================
# Memory Configuration
# ============================================

# Memory backend (qdrant or faiss)
A0_MEMORY_BACKEND=qdrant

# Memory subdirectory
MEMORY_SUBDIR=unity

# ============================================
# Performance Tuning
# ============================================

# Batch size for memory operations
MEMORY_BATCH_SIZE=100

# Memory cache size (number of entries)
MEMORY_CACHE_SIZE=10000

# Embedding cache size
EMBEDDING_CACHE_SIZE=50000

# ============================================
# System Configuration
# ============================================

# Timezone settings
DEFAULT_USER_UTC_OFFSET_MINUTES=-180
DEFAULT_USER_TIMEZONE=America/Sao_Paulo

# Runtime ID (auto-generated, don't change)
# A0_PERSISTENT_RUNTIME_ID=

# ============================================
# Google Cloud (if using Vertex AI)
# ============================================

VERTEX_PROJECT=your_project_id
VERTEX_LOCATION=us-central1
